{
  "ConnectionStrings": {
    "DefaultConnection": "DATABASE_SERVER_HERE"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DatabaseConnection": {
    "Server": "DATABASE_SERVER_HERE",
    "Database": "DATABASE_NAME_HERE",
    "UseWindowsAuth": false,
    "Username": "USERNAME_HERE",
    "Password": "PASSWORD_HERE"
  },
  "GraphAPI": {
    "login_endpoint": "https://login.microsoftonline.com",
    "tenant": "TENANT_HERE", //ID of the company
    "client_id": "CLIENT_ID_HERE", //ID from the app registered in Azure
    "scope": "https://graph.microsoft.com/.default",
    "client_secret": "CLIENT_SECRET_HERE", //Secret will expire after 2 years
    "grant_type": "client_credentials",
    "send_email_endpoint": "https://graph.microsoft.com/v1.0/users/EMAIL_ADDRESS_HERE/sendMail"
  },
  "APIEndpoint": "https://localhost:7236", //Test
  //"APIEndpoint": "https://SOMESITE.ac.uk",
  "AllowedHosts": "*",
  "EmailService": "GraphAPI",
  "Settings": {
    "DefaultAcademicYearID": "24/25",
    "EmailKey": "EMAIL_KEY_HERE"
  }
}